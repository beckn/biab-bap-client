---
server:
  port: {{ service_port }}
logging:
  level:
    org:
      apache:
        coyote:
          http11: DEBUG
spring:
  jackson:
    property-naming-strategy: SNAKE_CASE
    default-property-inclusion: non_null
registry_service:
  url: {{ registry_url }}
bpp_registry_service:
  url: https://demo4460370.mockable.io/bpp_registry/
protocol_service:
  url: {{ protocol_url }}
context:
  domain: nic2004:52110
  city: std:080
  country: IND
  bap_id: box.beckn.org
  bap_uri: {{ protocol_url }}protocol/v1
database:
  mongo:
    url: mongodb://{{ mongo_host }}:{{ mongo_port }}
    name: {{ bap_mongo_name }}
management:
  endpoints:
    web:
      exposure:
        include: "configprops, env, health, loggers, metrics, mappings, httptrace"
